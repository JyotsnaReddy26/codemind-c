a=int(input("enter side"))
print("area of square",a*a)
b=int(input("enter base:"))
c=int(input("enter height:"))
print("area of Rhobus:",b*c)
d=int(input("enter radius:"))
print("area of circle:",3.14*d*d)
print("area of sphere:",4*3.14*d*d)
print("perimeter of circle:",2*3.14*d)
e=int(input("enter number1:"))
f=int(input("enter number2:"))
print("mod value:",e%f)
print("div value:",e/f)
print(" double div value:",e//f)

