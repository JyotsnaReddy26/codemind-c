import java.util.Scanner;
class perimeterofrectangle
{
   public static void main(String args[])
{
   Scanner sc=new Scanner(System.in);
        int a,b,perimeter;
         a=sc.nextInt();
         b=sc.nextInt();
        perimeter=2*(a+b);
   System.out.println("perimeter of rectangle:"+perimeter);
}
}
